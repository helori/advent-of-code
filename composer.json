{
    "name": "helori/aoc_2021",
    "type": "project",
    "description": "Advent without chocolate",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/framework": "^8.40",
        "nesbot/carbon": "^2.55"
    },
    "autoload": {
        "psr-4": {
            "Aoc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
